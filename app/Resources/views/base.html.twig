<!DOCTYPE html>
<html>
<head>
    {% block stylesheet_libs %}
        {% stylesheets filter='cssrewrite'
        '@bootstrap' %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
    {% endblock %}

    {% block stylesheets %}
        {% stylesheets filter='cssrewrite'
            'css/style.css' %}
            <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
    {% endblock %}

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    <link rel="icon" type="image/png" href="{{ asset('favicon-32x32.png') }}" sizes="32x32"/>
    <link rel="icon" type="image/png" href="{{ asset('favicon-16x16.png') }}" sizes="16x16"/>

    <meta name="viewport" content="width=360"/>
    <meta charset="UTF-8"/>

    <title>Feedback App</title>
</head>

<body class="{% if container_class is defined %}{{ container_class }}{% endif %}">
{% block body %}
    <!-- header -->
    <header class="col-md-12">
        {% block header %}
        {% endblock %}
    </header>
    <!-- end header -->

    <!-- content -->
    {% block content %}
    {% endblock %}
    <!-- end content -->

    <!-- footer -->
    {% block footer %}
        <footer class="col-md-12">
        </footer>
    {% endblock %}
    <!-- end footer -->
{% endblock %}

{% block javascript_libs %}
    {% javascripts output='compiled/js/libs.js' filter="?jsqueeze"
        '@jquery'
        '@bootstrap_js'
        '@jquery_validate'
    %}
    <script src="{{ asset_url }}"></script>

{% endjavascripts %}

{% endblock %}

{% block javascripts %}
{% endblock %}
</body>
</html>
